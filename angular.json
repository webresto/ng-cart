{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@webresto/ng-cart": {
      "projectType": "library",
      "root": "projects/webresto/ng-cart",
      "sourceRoot": "projects/webresto/ng-cart/src",
      "prefix": "rst",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/webresto/ng-cart/tsconfig.lib.json",
            "project": "projects/webresto/ng-cart/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/webresto/ng-cart/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/webresto/ng-cart/src/test.ts",
            "tsConfig": "projects/webresto/ng-cart/tsconfig.spec.json",
            "karmaConfig": "projects/webresto/ng-cart/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/webresto/ng-cart/tsconfig.lib.json",
              "projects/webresto/ng-cart/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@webresto/ng-cart"
}
